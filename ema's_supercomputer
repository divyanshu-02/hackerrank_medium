#include <bits/stdc++.h>

using namespace std;
int calculategrid(char a[15][15],int n,int m,int k1,int k2)
{
    int l=1;
    int i1=k1-1,i2=k1+1,j1=k2-1,j2=k2+1;
    int i=k1;
    int j=k2;
    while(i1>=0&&i2<n&&j1>=0&&j2<m)
    {
        if(a[i1][j]=='B'||a[i2][j]=='B'||a[i][j1]=='B'||a[i][j2]=='B')
        {
            if(l%2!=0)
                return l;
            else
                return 0;    
        }   
        else
        {   
            l=l+2; 
            i1--;
            i2++;
            j1--;
            j2++;
        }        
    }
    return l;
}
void regrid(char a[15][15],char b[15][15],int n,int m)
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            b[i][j]=a[i][j];
        }
    }
}
int main()
{
    int n,m,max=0,k1,k2;
    cin>>n>>m;
    int i,j;
    char a[15][15],b[15][15];
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            cin>>a[i][j];
            b[i][j]=a[i][j];
        }
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            if(a[i][j]=='G')
            {
                int temp=calculategrid(a,n,m,i,j);
                if(max<temp)
                {
                    k1=i;
                    k2=j;
                    max=temp;
                }    
            }    
            
        }
    }
    int s=max/2;
    a[k1][k2]='B';
    int i1=k1-1,i2=k1+1,j1=k2-1,j2=k2+1;
    while(s>0)
    {
        a[i1][k2]='B';
        a[i2][k2]='B';
        a[k1][j1]='B';
        a[k1][j2]='B';
        i1--;
        i2++;
        j1--;
        j2++;
        s--;
    }
    int max1=0;
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            int temp=calculategrid(a,n,m,i,j);
            if(max1<temp)
            {
                max1=temp;
            }
        }
    }
    cout<<(max*2-1)*(max1*2-1);
}
